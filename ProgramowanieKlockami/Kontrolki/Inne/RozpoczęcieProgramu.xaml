<k:KlocekPionowyKontrolka x:Class="ProgramowanieKlockami.Kontrolki.Inne.RozpoczęcieProgramu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:k="clr-namespace:ProgramowanieKlockami.Kontrolki"
             mc:Ignorable="d">
    <Grid Background="White" Style="{StaticResource GridKlocka}">
        <Border BorderBrush="Black" Style="{StaticResource ObramowanieKlocka}">
            <Grid>
                <Ellipse Style="{StaticResource DolnyCypek}" Fill="Black"></Ellipse>
                <StackPanel>
                    <Label Foreground="Black" BorderBrush="Black">Start</Label>
                </StackPanel>
            </Grid>
        </Border>
    </Grid>
</k:KlocekPionowyKontrolka>