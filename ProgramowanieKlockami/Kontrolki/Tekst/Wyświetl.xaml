<k:KlocekPionowyPrzyjmującyWartośćKontrolka x:Class="ProgramowanieKlockami.Kontrolki.Tekst.Wyświetl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:k="clr-namespace:ProgramowanieKlockami.Kontrolki"
             mc:Ignorable="d">
    <Grid Background="LightGreen" Style="{StaticResource GridKlockaZGórnąDziurką}">
        <Border BorderBrush="Green" Style="{StaticResource ObramowanieKlocka}">
            <Grid>
                <Ellipse Fill="Green" Style="{StaticResource DolnyCypek}"></Ellipse>
                <Ellipse Fill="Green" Style="{StaticResource PrawyCypek}"></Ellipse>
                <Label>Wyświetl</Label>
            </Grid>
        </Border>
    </Grid>
</k:KlocekPionowyPrzyjmującyWartośćKontrolka>
