<k:KlocekZwracającyWartośćZawierającyListęZmiennychKontrolka x:Class="ProgramowanieKlockami.Kontrolki.Zmienne.WartośćZmiennej"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:k="clr-namespace:ProgramowanieKlockami.Kontrolki"
             mc:Ignorable="d">
    <Grid Style="{StaticResource GridKlockaZLewąDziurką}" Background="LightPink">
        <Border Style="{StaticResource ObramowanieKlocka}" BorderBrush="Red">
            <ComboBox Style="{StaticResource ListaZmiennych}"></ComboBox>
        </Border>
    </Grid>
</k:KlocekZwracającyWartośćZawierającyListęZmiennychKontrolka>