               BasedOn="{StaticResource RozwijanaListaKlocka}"
               TargetType="ComboBox">
            <Setter Property="ItemsSource"
                    Value="{Binding Source={StaticResource GłównyModelWidoku}, Path=Zmienne}"></Setter>
            <Setter Property="DisplayMemberPath"
                    Value="Nazwa"></Setter>
        </Style>
        <Style x:Key="MiejsceNaBocznyCypekKlockaPionowego"
               TargetType="Grid">
            <Setter Property="OpacityMask">
                <Setter.Value>
                    <DrawingBrush AlignmentX="Left"
                                  AlignmentY="Top"
                                  Stretch="None">
                        <DrawingBrush.Drawing>
                            <DrawingGroup>
                                <GeometryDrawing Brush="Black">
                                    <GeometryDrawing.Geometry>
                                        <CombinedGeometry GeometryCombineMode="Exclude">
                                            <CombinedGeometry.Geometry1>
                                                <RectangleGeometry Rect="0,0,99999999,99999999"></RectangleGeometry>
                                            </CombinedGeometry.Geometry1>
                                            <CombinedGeometry.Geometry2>
                                                <RectangleGeometry Rect="0,5,5,8"></RectangleGeometry>
                                            </CombinedGeometry.Geometry2>
                                        </CombinedGeometry>
                                    </GeometryDrawing.Geometry>
                                </GeometryDrawing>
                            </DrawingGroup>
                        </DrawingBrush.Drawing>
                    </DrawingBrush>
                </Setter.Value>
            </Setter>
        </Style>
        <Style x:Key="MiejsceNaDolnyCypekKlockaPionowego"
               TargetType="Grid">
