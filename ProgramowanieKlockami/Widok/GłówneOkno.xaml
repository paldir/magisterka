        <Style x:Key="BocznyCypekKlockaPrzyjmującegoWartość"
               TargetType="Rectangle">
            <Setter Property="Height"
                    Value="6"></Setter>
            <Setter Property="HorizontalAlignment"
                    Value="Right"></Setter>
            <Setter Property="RenderTransform">
                <Setter.Value>
                    <TranslateTransform X="5"
                                        Y="6"></TranslateTransform>
                </Setter.Value>
            </Setter>
            <Setter Property="VerticalAlignment"
                    Value="Top"></Setter>
            <Setter Property="Width"
                    Value="5"></Setter>
        </Style>
        <Style x:Key="BocznyCypekWewnętrzny"
               BasedOn="{StaticResource BocznyCypekKlockaPrzyjmującegoWartość}"
               TargetType="Rectangle">
            <Setter Property="HorizontalAlignment"
                    Value="Left"></Setter>
            <Setter Property="RenderTransform">
                <Setter.Value>
                    <TranslateTransform X="1"
                                        Y="8"></TranslateTransform>
                </Setter.Value>
            </Setter>
        </Style>
        <Style x:Key="DolnyCypekKlockaPionowego"
               TargetType="Rectangle">
            <Setter Property="Height"
                    Value="5"></Setter>
            <Setter Property="HorizontalAlignment"
                    Value="Left"></Setter>
            <Setter Property="RenderTransform">
                <Setter.Value>
                    <TranslateTransform X="6"
                                        Y="5"></TranslateTransform>
                </Setter.Value>
            </Setter>
            <Setter Property="VerticalAlignment"
                    Value="Bottom"></Setter>
            <Setter Property="Width"
                    Value="6"></Setter>
        </Style>
